# TrustBridge Contracts Environment Variables
# Copy this file to .env and fill in your values

# Alchemy API Key for Ethereum RPC access
ALCHEMY_API_KEY=your-alchemy-api-key-here

# Private key for contract deployment (without 0x prefix)
WALLET_PRIVATE_KEY=your-wallet-private-key-here

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your-etherscan-api-key

# Network URLs
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/${ALCHEMY_API_KEY}
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}